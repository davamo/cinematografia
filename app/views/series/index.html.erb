<div class="container"> 
    <h1>Lista de Series</h1>
    <ul>
    <% @series.each do |serie| %>
        <li>
        <strong>Nombre:</strong> <%= serie.name %><br>
        <strong>Sinopsis:</strong> <%= serie.synopsis %><br>
        <strong>Director:</strong> <%= serie.director %>
        </li>
    <% end %>
    </ul>
</div>